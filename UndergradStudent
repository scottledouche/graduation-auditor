public class UndergradStudent extends Student implements Auditable{
  
	private int commServ;
	
	public UndergradStudent(String f, String l, int id) {
		super(f, l, id);
	}

	@Override
	public boolean meetsRequirements() {

		if (creditsCompleted >= 30 && commServ >= 20)
			return true;		
		else 
			return false;	
	}
	
	public void communityService (int cs) {
		System.out.println(getFirstName() + " " + getLastName() + " completed " + commServ + " hours of community service");
	}
		
	public void takeCourse (int courseCredits, String courseName, int hours){
	    creditsCompleted += courseCredits;
	    hours += commServ;
	}	
}
